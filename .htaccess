Options -Indexes

# Prod
RewriteEngine On
RewriteCond %{REQUEST_URI} !^/blooddonate
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /index.php?$1 [L]


# Local/Demo
RewriteEngine On
RewriteCond %{REQUEST_URI} ^/blooddonate
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /blooddonate/index.php?$1 [L]

